                border_radius: 20
                radius: [15]
                spacing: "12dp"
        
                MDLabel:
                    
                    text:'Por Nome'
                    halign: 'center'
                    theme_text_color: "Custom"
                    font_size: 25 
                    md_bg_color: app.theme_cls.primary_light  
